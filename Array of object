#include<iostream>
using namespace std;
class partha
{
	public:
		    char name[40];
		    int age;
		    
	public:
		    void getdata()
		    {
		    	cout<<"Enter name-";
		    	cin>>name;
		    	cout<<"\nEnter age-\n";
		    	cin>>age;
			}
			
			void putdata()
			{
				cout<<"\nName-"<<name;
				cout<<"\nAge-"<<age;
			}
			
		
};


int main()
{
	partha p[3];
	for(int i=0;i<3;i++)
	{
		p[i].getdata();
	}
	
	for(int i=0;i<3;i++)
	{
		p[i].putdata();
	}
	return 0;
}
